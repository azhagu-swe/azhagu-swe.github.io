{
    "ci": {
        "collect": {
            "startServerCommand": "npx -y serve@latest out -p 3000",
            "url": [
                "http://localhost:3000/"
            ],
            "numberOfRuns": 3
        },
        "assert": {
            "assertions": {
                "categories:performance": [
                    "warn",
                    {
                        "minScore": 0.9
                    }
                ],
                "categories:accessibility": [
                    "error",
                    {
                        "minScore": 0.8
                    }
                ],
                "categories:best-practices": [
                    "warn",
                    {
                        "minScore": 0.9
                    }
                ],
                "categories:seo": [
                    "error",
                    {
                        "minScore": 0.8
                    }
                ]
            }
        },
        "upload": {
            "target": "temporary-public-storage"
        }
    }
}